                        <VBox>
                           <children>
                              <VBox alignment="TOP_CENTER" prefHeight="847.0" spacing="20.0" styleClass="content-background" stylesheets="@../../../../resources/styles.css">
                                 <children>
                                    <HBox>
                                       <children>
                                          <VBox prefHeight="82.0" spacing="10.0" HBox.hgrow="ALWAYS" VBox.vgrow="NEVER">
